@article{RefWorks:RefID:62-ning2015additive,
  author   = {Fuda Ning and Weilong Cong and Jingjing Qiu and Junhua Wei and Shiren Wang},
  year     = {2015},
  month    = {-06-20},
  title    = {Additive manufacturing of carbon fiber reinforced thermoplastic composites using fused deposition modeling},
  journal  = {Composites Part B: Engineering},
  volume   = {80},
  note     = {Key Point:<br>- To combine materials before extruding filament, using a blender may be required},
  abstract = {Additive manufacturing (AM) technologies have been successfully applied in various applications. Fused deposition modeling (FDM), one of the most popular AM techniques, is the most widely used method for fabricating thermoplastic parts those are mainly used as rapid prototypes for functional testing with advantages of low cost, minimal wastage, and ease of material change. Due to the intrinsically limited mechanical properties of pure thermoplastic materials, there is a critical need to improve mechanical properties for FDM-fabricated pure thermoplastic parts. One of the possible methods is adding reinforced materials (such as carbon fibers) into plastic materials to form thermoplastic matrix carbon fiber reinforced plastic (CFRP) composites those could be directly used in the actual application areas, such as aerospace, automotive, and wind energy. This paper is going to present FDM of thermoplastic matrix CFRP composites and test if adding carbon fiber (different content and length) can improve the mechanical properties of FDM-fabricated parts. The CFRP feedstock filaments were fabricated from plastic pellets and carbon fiber powders for FDM process. After FDM fabrication, effects on the tensile properties (including tensile strength, Young's modulus, toughness, yield strength, and ductility) and flexural properties (including flexural stress, flexural modulus, flexural toughness, and flexural yield strength) of specimens were experimentally investigated. In order to explore the parts fracture reasons during tensile and flexural tests, fracture interface of CFRP composite specimens after tensile testing and flexural testing was observed and analyzed using SEM micrograph.},
  keywords = {Composite; Combining Materials; Extrusion},
  isbn     = {1359-8368},
  doi      = {10.1016/j.compositesb.2015.06.013}
}
@misc{RefWorks:RefID:60-ailenei2025study,
  author   = {Eugen Constantin Ailenei and Savin Dorin Ionesi and Emil Constantin Loghin and Ionut Dulgheriu},
  year     = {2025},
  month    = {-02-07},
  title    = {Study of the Shredding Parameters of Pla Waste Used in Making Filaments for {3D} Printing},
  journal  = {The 19th Romanian Textiles and Leather Conference},
  note     = {Key points:\&nbsp;<br>- Filtering for size of regrind (1.6-2mm) is crucial for filament quality<br>- Ideal filament diameter is 1.75 +/- 0.05mm (generally accepted tolerance)},
  abstract = {Due to the simplicity and accessibility of the 3D printing FDM (Fused Deposition Modeling) technology, it has gained many followers, turning into a real industry. Thus, with the appearance of numerous manufacturers of 3D printers, they have become affordable serial products. Access to numerous open source CAD design programs and databases with ready-to-use projects have allowed the use of technology in production applications and prototyping in the most diverse fields, but also for the creation of final products for household use. The most commonly used thermoplastic materials in 3D printing are polylactic acid (PLA), acrylonitrile butadiene styrene (ABS), polyethylene terephthalate (PET), polypropylene (PP), thermoplastic urethane (TPU), and nylon [1]. Among them, polylactic acid stands out, being a biodegradable and recyclable polymer with multiple applications and the most used filament for 3D printing [2]. The judicious use of biodegradable raw materials contributes significantly to the effort to reduce the impact on the environment and to sustainable development. The increasingly widespread use of 3D printing also involves the generation of increasing amounts of PLA waste, and by recycling and reducing it we can contribute to lowering costs, reducing the demand for virgin raw materials and reducing the environmental impact. In this paper we will study the influence of the shredding dimensions of PLA waste prepared for extrusion on the characteristics of the obtained filament.},
  keywords = {Extrusion; Regrind; Shredding},
  doi      = {10.2478/9788367405829-048}
}
