  title    = {Biodegradable highly porous interconnected poly(ε‐caprolactone)/poly(L‐lactide‐co‐ε‐caprolactone) scaffolds by supercritical foaming for small‐diameter vascular tissue engineering},
  journal  = {Polymers for Advanced Technologies},
  volume   = {33},
